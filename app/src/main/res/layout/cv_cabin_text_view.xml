<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <TextView
        android:id="@+id/cabinTextView_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="start"
        android:layout_weight="0"
        android:text="1,3"
        android:textAppearance="?android:textAppearanceLarge"
        android:textStyle="bold"
        tools:text="1,3" />

    <TextView
        android:id="@+id/cabinTextView_address"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_weight="1"
        android:gravity="center"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:text="5 شارع عبد الفتاح السيسي يبببسنبىتيستنبىريبتنىربني"
        android:textAppearance="?android:textAppearanceMedium"
        tools:text="5 شارع عبد الفتاح السيسي" />

    <TextView
        android:id="@+id/cabinTextView_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:layout_weight="0"
        android:text="@string/copper"
        android:textAppearance="?android:textAppearanceLarge"
        android:textStyle="bold"
        tools:text="COPPER" />


</merge>